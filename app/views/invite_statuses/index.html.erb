<h1>Listing invite_statuses</h1>

<table>
  <tr>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invite_statuses.each do |invite_status| %>
  <tr>
    <td><%= invite_status.status %></td>
    <td><%= link_to 'Show', invite_status %></td>
    <td><%= link_to 'Edit', edit_invite_status_path(invite_status) %></td>
    <td><%= link_to 'Destroy', invite_status, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

