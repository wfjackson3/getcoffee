<h1>Listing event_statuses</h1>

<table>
  <tr>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @event_statuses.each do |event_status| %>
  <tr>
    <td><%= event_status.status %></td>
    <td><%= link_to 'Show', event_status %></td>
    <td><%= link_to 'Edit', edit_event_status_path(event_status) %></td>
    <td><%= link_to 'Destroy', event_status, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event status', new_event_status_path %>
